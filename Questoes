1) def fibonacci_generator():
    a, b = 0, 1
    while True:
        yield a
        a, b = b, a + b

def check_fibonacci(n):
    for fib in fibonacci_generator():
        if fib == n:
            return True
        if fib > n:
            return False

numero = int(input("Informe um número: "))
if check_fibonacci(numero):
    print(f"O número {numero} pertence à sequência de Fibonacci.")
else:
    print(f"O número {numero} não pertence à sequência de Fibonacci.")


2) def contar_letra_a(string):

       string_lower = string.lower()

       quantidade_a = string_lower.count('a')

        if quantidade_a > 0:
        return f"A letra 'a' aparece {quantidade_a} vez(es) na string."
    else:
        return "A letra 'a' não está presente na string."

texto = input("Informe uma string: ")
resultado = contar_letra_a(texto)
print(resultado)


